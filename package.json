{
  "name": "android-publish",
  "description": "Push Android Bundles to Google Play Store from cli",
  "author": "Hausenn @HausennTechnologies",
  "version": "1.0.11",
  "license": "MIT",
  "homepage": "https://github.com/HausennTechnologies/android-publish",
  "repository": "https://github.com/HausennTechnologies/android-publish",
  "bugs": "https://github.com/HausennTechnologies/android-publish",
  "keywords": [
    "ci",
    "google play",
    "android",
    "mobile",
    "bundle"
  ],
  "scripts": {
    "prepare": "husky install",
    "prepack": "rm -rf lib && tsc -b",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "eslint": "eslint . --ext .ts --config .eslintrc",
    "prettier": "npx prettier \"**/*.ts\" --check",
    "lint": "npm run eslint && npm run prettier",
    "semantic-release": "semantic-release"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/bin",
    "/lib"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "bin": {
    "android-publish": "./bin/run"
  },
  "oclif": {
    "bin": "android-push"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@types/sinon": "^10.0.2",
    "cli-ux": "^5.5.1",
    "googleapis": "^76.0.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.1",
    "@semantic-release/npm": "^7.1.1",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^5.2.7",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^16.6.2",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.4",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "mocha": "^5.2.0",
    "mock-fs": "^5.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.2",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.6.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  }
}
